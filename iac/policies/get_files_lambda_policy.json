{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion",
        "s3:ListBucket",
        "s3:ListBucketVersions"
      ],
      "Resource": [
        "arn:aws:s3:::${FILES_BUCKET_ID}",
        "arn:aws:s3:::${FILES_BUCKET_ID}/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "kms:GenerateDataKey",
        "kms:Decrypt"
      ],
      "Resource": [
        "${FILES_BUCKET_KMS_ARN}"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "dynamodb:GetItem"
      ],
      "Resource": [
        "${HAMPI_UPLOADS_TABLE_ARN}",
        "${HAMPI_UPLOADS_TABLE_ARN}/*"
      ]
    }
  ]
}
